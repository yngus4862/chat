{
  "Api": {
    "BaseUrl": "http://localhost:8080"
  },
  "Realtime": {
    "WsBaseUrl": "ws://localhost:8081",
    "Path": "/ws"
  },
  "Auth": {
    "Enabled": false,
    "Issuer": "https://<keycloak-host>/realms/<realm>",
    "ClientId": "<client-id>",
    "RedirectUri": "mychat://callback",
    "Scope": "openid profile email offline_access"
  },
  "Notifications": {
    "ThrottleWindowSeconds": 8,
    "BurstCount": 3
  }
}